{
  "buildOnSave": false,
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "isolatedModules": false,
    "jsx": "react",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true
  },
  "filesGlob": [
    "**/*.ts",
    "**/*.tsx",
    "!**/*.js",
    "!node_modules/**"
  ],
  "files": [
    "android/ref.ts",
    "index.android.ts",
    "index.ios.ts"
  ],
  "exclude": []
}
